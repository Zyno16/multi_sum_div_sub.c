#include <stdio.h>
#include <stdlib.h>
#include<math.h>

int main()
{
    float x,y,sum,multi,div,sub;
    printf("enter x?:\n");
    scanf("%.2f",&x);
    printf("enter y?:\n);
    scanf("%f",&y);
    sum=x+y;
    multi=x*y;
    div=x/y;
    sub=x-y;
    printf("the sum is  :%0.2f",sum);
    printf("the multi is:%.2f",multi);
    printf("the div is  :%.2f",div);
    printf("the sub is  :%.2f",sub);

    return 0;
}
